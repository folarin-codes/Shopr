{
  "expo": {
    "name": "Shopr",
    "slug": "shopr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "shopr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shopr"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "black"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.shopr"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ivon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "black"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "51f49769-2f7f-4e26-a571-63b150f34fe3"
      }
    },
    "owner": "adefolarin"
  }
}
